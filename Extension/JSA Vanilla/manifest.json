{
	"name": "JS Static Analysis (Vanilla)",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Prevents the user from malicious JS code",
	"browser_action": {
		"default_icon": "icon.png"
	},

	"background": {
		"scripts": ["background.js"]
	},
	
	"permissions": ["tabs", "<all_urls>", "background", "storage"]
}